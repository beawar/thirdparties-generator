{
  "name": "thirdparties-generator",
  "version": "0.0.0-development",
  "description": "Thirdparties file generator for node projects",
  "main": "dist/index.js",
  "module": "esm/index.js",
  "bin": {
    "thirdparties-generator": "esm/index.js"
  },
  "files": [
    "dist",
    "esm",
    "src",
    "config"
  ],
  "scripts": {
    "lint": "eslint",
    "lint:fix": "eslint --fix",
    "type-check": "tsc --noEmit",
    "start": "ts-node src/index.ts",
    "clean": "rimraf dist esm",
    "build:cjs": "tsc --module commonjs --outDir dist",
    "build:esm": "tsc --target esnext --outDir esm",
    "build": "npm run build:esm && npm run build:cjs",
    "prebuild": "npm run clean",
    "prepack": "npm run build",
    "preversion": "npm run build",
    "postversion": "git push --follow-tags",
    "prepare": "husky install",
    "semantic-release": "semantic-release"
  },
  "keywords": [],
  "author": "Beatrice Guerra <betrix.dev@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@commitlint/cli": "^17.4.4",
    "@commitlint/config-conventional": "^17.4.4",
    "@tsconfig/recommended": "^1.0.2",
    "@types/lodash": "^4.14.191",
    "@types/node": "^20.1.4",
    "@types/yargs": "^17.0.22",
    "@typescript-eslint/eslint-plugin": "^5.59.5",
    "@typescript-eslint/parser": "^5.59.6",
    "eslint": "^8.40.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-unused-imports": "^2.0.0",
    "husky": "^8.0.3",
    "rimraf": "^5.0.0",
    "semantic-release": "^21.0.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.4"
  },
  "dependencies": {
    "license-checker-rseidelsohn": "^4.2.1",
    "lodash": "^4.17.21",
    "yargs": "^17.7.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/beawar/thirdparties-generator.git"
  }
}

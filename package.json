{
  "name": "thirdparties-generator",
  "version": "0.0.0-development",
  "description": "Thirdparties file generator for node projects",
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "bin": {
    "thirdparties-generator": "dist/index.js"
  },
  "files": [
    "dist",
    "src",
    "config"
  ],
  "scripts": {
    "lint": "eslint",
    "lint:fix": "eslint --fix",
    "type-check": "tsc --noEmit",
    "prestart": "npm run build",
    "start": "node dist/index.js",
    "clean": "rimraf dist",
    "build": "tsc",
    "prebuild": "npm run clean",
    "prepack": "npm run build",
    "preversion": "npm run build",
    "postversion": "git push --follow-tags",
    "prepare": "husky install",
    "semantic-release": "semantic-release"
  },
  "keywords": [],
  "author": "Beatrice Guerra <betrix.dev@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@commitlint/cli": "^19.3.0",
    "@commitlint/config-conventional": "^19.2.2",
    "@tsconfig/recommended": "^1.0.6",
    "@types/node": "^20.14.2",
    "@types/yargs": "^17.0.32",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^7.14.1",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-unused-imports": "^3.0.0",
    "husky": "^9.0.11",
    "rimraf": "^5.0.7",
    "semantic-release": "^24.0.0",
    "typescript": "^5.4.5"
  },
  "dependencies": {
    "license-checker-rseidelsohn": "4.3.0",
    "yargs": "^17.7.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/beawar/thirdparties-generator.git"
  },
  "engines": {
    "node": "^18 || ^20"
  }

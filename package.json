{
    "name": "thirdparties-generator",
    "version": "0.0.0-development",
    "description": "Thirdparties file generator for node projects",
    "type": "module",
    "main": "dist/index.js",
    "module": "dist/index.js",
    "bin": {
        "thirdparties-generator": "dist/index.js"
    },
    "files": [
        "dist",
        "src",
        "config"
    ],
    "scripts": {
        "lint": "eslint",
        "lint:fix": "eslint --fix",
        "format": "prettier . --write",
        "format:check": "prettier . --check",
        "type-check": "tsc --noEmit",
        "prestart": "npm run build",
        "start": "node dist/index.js",
        "clean": "rimraf dist",
        "build": "tsc",
        "prebuild": "npm run clean",
        "prepack": "npm run build",
        "preversion": "npm run build",
        "postversion": "git push --follow-tags",
        "prepare": "husky",
        "semantic-release": "semantic-release"
    },
    "keywords": [],
    "author": "Beatrice Guerra <betrix.dev@gmail.com>",
    "license": "MIT",
    "devDependencies": {
        "@commitlint/cli": "^19.5.0",
        "@commitlint/config-conventional": "^19.5.0",
        "@eslint/js": "^9.10.0",
        "@types/eslint__js": "^8.42.3",
        "@types/eslint-config-prettier": "^6.11.3",
        "@types/node": "^20.14.2",
        "@types/yargs": "^17.0.33",
        "eslint": "^9.10.0",
        "eslint-config-prettier": "^9.1.0",
        "eslint-plugin-unused-imports": "^4.1.4",
        "globals": "^15.9.0",
        "husky": "^9.1.6",
        "prettier": "3.3.3",
        "rimraf": "^6.0.1",
        "semantic-release": "^24.1.1",
        "typescript": "~5.6.3",
        "typescript-eslint": "^8.6.0"
    },
    "dependencies": {
        "license-checker-rseidelsohn": "4.4.2",
        "yargs": "^17.7.2"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/beawar/thirdparties-generator.git"
    },
    "engines": {
        "node": "^18 || ^20"
    }
}

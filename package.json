{
  "name": "thirdparties-generator",
  "version": "0.0.0",
  "description": "Thirdparties file generator for node projects",
  "main": "dist/index.js",
  "module": "esm/index.js",
  "bin": "dist/index.js",
  "files": [
    "dist",
    "esm",
    "src"
  ],
  "scripts": {
    "lint": "eslint",
    "lint:fix": "eslint --fix",
    "start": "ts-node src/index.ts",
    "clean": "rimraf lib",
    "copy-files": "copyfiles config/* dist; copyfiles config/* esm",
    "build:cjs": "tsc --module commonjs --outDir dist",
    "build:esm": "tsc --target esnext --outDir esm",
    "build": "npm run build:esm && npm run build:cjs",
    "postbuild": "npm run copy-files",
    "prebuild": "npm run clean",
    "prepack": "npm run build",
    "preversion": "npm run build",
    "postversion": "git push --follow-tags"
  },
  "keywords": [],
  "author": "Beatrice Guerra <betrix.dev@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@tsconfig/recommended": "^1.0.2",
    "@types/lodash": "^4.14.191",
    "@types/node": "^18.14.2",
    "@types/yargs": "^17.0.22",
    "@typescript-eslint/eslint-plugin": "^5.54.0",
    "@typescript-eslint/parser": "^5.54.0",
    "copyfiles": "^2.4.1",
    "eslint": "^8.35.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-prettier": "^4.2.1",
    "rimraf": "^4.1.2",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5"
  },
  "dependencies": {
    "license-checker-rseidelsohn": "github:beawar/license-checker-rseidelsohn",
    "lodash": "^4.17.21",
    "yargs": "^17.7.1"
  }
}
